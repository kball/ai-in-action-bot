{
  "name": "start-light-express",
  "version": "1.0.0",
  "main": "server.js",
  "bin": {
    "aiia-chat": "bin/aiia-chat"
  },
  "scripts": {
    "start": "npm run build && node index.js",
    "dev": "nodemon --exec \"npm start\"",
    "build": "npm run deploy-commands",
    "deploy-commands": "node lib/discord/deploy-commands.js",
    "lint": "prettier-eslint --write --no-semi --single-quote \"**/*.js\"",
    "test": "NODE_ENV=test node test/index.js",
    "chat": "node bin/aiia-chat",
    "test-proactive": "node bin/test-proactive.js"
  },
  "keywords": [
    "express",
    "starter",
    "template",
    "mongodb",
    "authentication",
    "testing",
    "production-ready"
  ],
  "author": "David Guttman <david@davidguttman.com> (http://davidguttman.com/)",
  "license": "MIT",
  "description": "A lightweight, illuminating Express.js starter template with MongoDB, authentication, and testing",
  "dependencies": {
    "@discordjs/rest": "^2.4.3",
    "authentic-service": "^1.0.0",
    "cuid": "^3.0.0",
    "deasync": "^0.1.30",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "healthpoint": "^1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "mongodb-memory-server": "^10.1.4",
    "mongoose": "^8.13.1",
    "openai": "^4.95.1",
    "productionize": "^5.0.0",
    "supertest": "^7.1.0",
    "tape": "^5.9.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "prettier-eslint-cli": "^8.0.1"
  }
}
